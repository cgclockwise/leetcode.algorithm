# leetcode.algorithm
class solution{
   public static int maxProfit(int [] prices){
    int max = 0;
    for(i=0;i<prices.length - 1;i++){
      if(prices[i+1]>prices[i])
        max += prices[i+1] - prices[i];
      }
    }  
    return max;
    
    public static void main(String [] args){
      int []test = {7,1,5,3,6,4};
		  System.out.println(maxProfit(test));
    }
}
